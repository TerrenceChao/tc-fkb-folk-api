# app
NODE_ENV=development
PORT=3000
ACCESS_CONTROL_ALLOW_ORIGIN=http://localhost:4200
ACCESS_CONTROL_ALLOW_METHODS=POST,GET,UPDATE,PUT,PATCH,DELETE,OPTIONS
ACCESS_CONTROL_ALLOW_HEADERS=xxxxxx,xxxxxx,xxxxxx,xxxxxx
PREFIX=/xxxxxxxxxxxxxxxxx

# http request
REQ_RETRY_LIMIT=3
REQ_DELAY=500
REQ_TIMEOUT=100
EXPIRATION_SECS=300
REGEX_UUID=/[0-9a-fA-F]{8}\/-[0-9a-fA-F]{4}\/-[0-9a-fA-F]{4}\/-[0-9a-fA-F]{4}\/-[0-9a-fA-F]{12}/

# infrastructure
SQL_USER=xxxxxx
SQL_PW=xxxxxx
SQL_DATABASE=xxxxxx
SQL_HOST=127.0.0.1
SQL_PORT=5432
SQL_POOL_MAX_SIZE=20
SQL_CONNECTION_IDLE_TIMEOUT=3000

# abount domains
FRIEND_BATCH_LIMIT=50
ACCOUT_IDENTITY=xxxxxx,xxxxxx,xxxxxx,xxxxxx
USER_PUBLIC_INFO=xxxxxx,xxxxxx,xxxxxx,xxxxxx
USER_PRIVATE_INFO=xxxxxx,xxxxxx,xxxxxx,xxxxxx
USER_CONTACT=xxxxxx,xxxxxx,xxxxxx,xxxxxx
USER_UPDATE_CONTACT=xxxxxx,xxxxxx,xxxxxx,xxxxxx
USER_VALIDATE_CONTACT=xxxxxx,xxxxxx,xxxxxx,xxxxxx
USER_COMMON_PUBLIC_INFO=xxxxxx,xxxxxx,xxxxxx,xxxxxx
USER_AUTHENTICATION=xxxxxx,xxxxxx,xxxxxx,xxxxxx
USER_VERIFICATION=xxxxxx,xxxxxx,xxxxxx,xxxxxx

# auth service
REGISTER_EXPIRATION=10
VERIFY_EXPIRATION=10


# message service
MESSAGING_HOST=http://localhost:3004 
MESSAGING_PATH_AUTHENTICATE=/service/path/xxxxxxxxxxxxxxxxx


# notification service
SPECIFY_NOTIFICATION_VENDOR=mq
# mq (message queue)
NOTIFICATION_MQ_HOST=http://localhost:9000
NOTIFICATION_MQ_PATH_PUBLISH=/service/path/xxxxxxxxxxxxxxxxx

# redis
NOTIFICATION_REDIS_HOST=127.0.0.1
NOTIFICATION_REDIS_PORT=6379


# front-end service
FRONTEND_HOST=http://www.fakebook.com
FRONTEND_PATH_RECOVER_BY_CODE=/service/path/xxxxxxxxxxxxxxxxx
FRONTEND_PATH_RECOVER_BY_PASSWORD=/service/path/xxxxxxxxxxxxxxxxx